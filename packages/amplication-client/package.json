    "lint": "eslint --cache .",
    "lint:fix": "npm run lint -- --fix",
    "check-format": "prettier --check \"**/*.{js,ts,json,gql,md,yaml}\"",
    "format": "prettier --write \"**/*.{js,ts,json,gql,md,yaml}\""
  },
  "proxy": "http://localhost:3000",
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
